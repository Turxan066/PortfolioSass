{
	"name": "vscode-autosave-ext",
	"displayName": "AutoSaveExt",
	"description": "",
	"version": "0.0.1",
	"publisher": "oonsnnsk",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "autoSaveExt.reloadConfig",
				"title": "AutoSaveExt: Reload settings"
			}
		],
		"configuration": {
			"type": "object",
			"title": "AutoSaveExt",
			"properties": {
				"autoSaveExt": {
					"debounce": {
						"type": "number",
						"default": 500,
						"description": "default is 500"
					},
					"extensions": {
						"type": "array",
						"description": "if no extension set will be applied to all files"
					}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"_pretest": "yarn run compile && yarn run lint",
		"test": "mocha ./out/test/*.test.js"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"prettier": "^2.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"xstream": "^11.14.0"
	},
	"license": "MIT",
